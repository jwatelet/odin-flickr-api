<%= form_with method: :get do |f| %>
  <%= f.label :user_id, "Flickr user_id:"  %>
  <%= f.text_field :user_id %>

  <%= f.submit "Search" %>
<% end %>


<% if @photos %>
  <ul>
    <% @photos.each do |photo| %>
      <li class="images">
        <%= image_tag "https://live.staticflickr.com/#{photo.server}/#{photo.id}_#{photo.secret}.jpg" %>
      </li>
    <% end %>
  </ul>
<% end %>